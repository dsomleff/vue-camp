{
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 120,
    "useTabs": false,
    "quoteProps": "as-needed",
    "trailingComma": "true",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "singleAttributePerLine": true
}
